{
  "name": "protobuf-array-stream",
  "version": "1.0.0",
  "description": "Stream and decode Protocol Buffer arrays without memory issues - perfect for processing large datasets",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "test": "ts-node test/index.test.ts",
    "coverage": "c8 npm test"
  },
  "keywords": [
    "protobuf",
    "stream",
    "memory-efficient",
    "streaming",
    "repeated-fields",
    "arrays",
    "large-datasets",
    "nodejs",
    "typescript",
    "protocol-buffers",
    "transform-stream",
    "repeated-fields"
  ],
  "author": "Akash S Panickar",
  "license": "MIT",
  "homepage": "https://github.com/mochatek/protobuf-array-stream",
  "repository": {
    "type": "git",
    "url": "https://github.com/mochatek/protobuf-array-stream.git"
  },
  "dependencies": {
    "protobufjs": "^7.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "c8": "10.1.3",
    "husky": "9.1.7",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "bugs": {
    "url": "https://github.com/mochatek/protobuf-array-stream/issues"
  }
}
